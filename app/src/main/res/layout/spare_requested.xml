                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/_10dp"
                                android:layout_weight=".5"
                                android:text="12-05-2019 02:29:PM"
                                android:textSize="@dimen/_16dp">

                            </TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight=".5"
                                android:text="Approval Date"
                                android:textStyle="bold"
                                android:textSize="@dimen/_16dp"
                              >

                            </TextView>

                            <TextView
                                android:id="@+id/approvalDate"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/_10dp"
                                android:layout_weight=".5"
                                android:text="Not Available"
                                android:textSize="@dimen/_16dp">

                            </TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight=".5"
                                android:text="Acknowledge Date By SF"
                                android:textStyle="bold"
                                android:textSize="@dimen/_16dp">

                            </TextView>

                            <TextView
                                android:id="@+id/acknowledgeDateBYSF"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/_10dp"
                                android:layout_weight=".5"
                                android:text="Not Available"
                                android:textSize="@dimen/_16dp">

                            </TextView>
                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight=".5"
                                android:text="Remarks By SC"
                                android:textStyle="bold"
                                android:textSize="@dimen/_16dp">

                            </TextView>

                            <TextView
                                android:id="@+id/remarksBySC"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/_10dp"
                                android:layout_weight=".5"
                                android:text="dead"
                                android:textSize="@dimen/_16dp">

                            </TextView>
                        </LinearLayout>

                    </LinearLayout>
                </androidx.cardview.widget.CardView>


                <LinearLayout
                    android:id="@+id/partStatus"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Part Status"
                        android:textColor="@color/colorAccent"
                        android:textStyle="bold"
                        android:textSize="@dimen/_18dp"
                       ></TextView>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:layout_marginLeft="@dimen/_10dp"
                        android:layout_weight="1">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_3dp"
                            android:layout_centerInParent="true"
                            android:background="@color/gray"></View>

                        <ImageView
                            android:id="@+id/statusExpandIcon"
                            android:layout_width="@dimen/_25dp"
                            android:layout_height="@dimen/_35dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/_30dp"
                            android:background="@drawable/downexpandicon">

                        </ImageView>
                    </RelativeLayout>

