                android:textColor="@color/colorAccent"
                android:textSize="@dimen/_18dp"
                ></TextView>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="end"
                android:layout_marginLeft="@dimen/_10dp"
