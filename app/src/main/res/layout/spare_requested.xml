                                android:layout_weight=".5"
                                android:text="Not Available"
                                android:textSize="@dimen/_16dp">

                            </TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10dp"
