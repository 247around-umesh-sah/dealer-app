<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/_8dp">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <androidx.cardview.widget.CardView
                android:id="@+id/partDetialCardView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5dp"
                android:layout_marginTop="@dimen/_5dp"
                android:layout_marginRight="@dimen/_5dp"
                android:layout_marginBottom="@dimen/_5dp"
                app:cardCornerRadius="@dimen/_10dp"
                app:cardElevation="5dp">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/_11dp"
                    android:paddingTop="@dimen/_5dp"
                    android:paddingRight="@dimen/_11dp"
                    android:paddingBottom="@dimen/_8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Spare ID "
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/spareID"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="32541"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/partShippedBy"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Part Shipped By"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/partner_warehouse"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="Warehouse"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Shipped Parts"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/shippedParts"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="COMPRESSOR,WITH ACCESSORIES LG-MA42LPJG"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Shipped Parts Number"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp" />

                        <TextView
                            android:id="@+id/shippedPartsNumber"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="1100096629"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Part Type"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/partType"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="COMPRESSOR"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Shipped Quantity"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/shippedQuantity"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="1"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Pickup Request"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/PickupRequest"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="Not Available"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Pickup Schedule"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/PickupSchedule"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="Not Available"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Courier Name"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/courierName"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="gati kwe"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="AWB"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/AWB"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="325683183"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/noOfBoxesLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="No. Of Boxes"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/NoOfBoxes"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="Not Available"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:textStyle="bold"
                            android:text="Weight"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/Weight"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="24 KG 000 Gram"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:textStyle="bold"
                            android:text="Shipped Date"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/shippeddate"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="11-06-2019"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/eddLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/edd"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="EDD"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/EDD"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="Not Available"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:textStyle="bold"
                            android:text="Remarks By Partner"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/RemarksByPartner"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="Part shipped to SF"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Challan Number"
                            android:textStyle="bold"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/ChallanNumber"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="GOWTVS DC 0001"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:textStyle="bold"
                            android:text="Challan Approx Value"
                            android:textSize="@dimen/_16dp">

                        </TextView>

                        <TextView
                            android:id="@+id/ChallanapproxValue"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="1971.25"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".5"
                            android:text="Is Defective/Ok Parts Required"
                            android:textSize="@dimen/_16dp"
                            android:textStyle="bold">

                        </TextView>

                        <TextView
                            android:id="@+id/IsDefectiveOkPartsRequired"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10dp"
                            android:layout_weight=".5"
                            android:text="Not Available"
                            android:textSize="@dimen/_16dp">

                        </TextView>
                    </LinearLayout>


                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <LinearLayout
                android:id="@+id/partTrack"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="5dp"
                android:paddingTop="@dimen/_10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Track Spare Part"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/_18dp"></TextView>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="end"
                    android:layout_marginLeft="@dimen/_10dp"
                    android:layout_weight="1">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_3dp"
                        android:layout_centerInParent="true"
                        android:background="@color/gray"></View>

                    <ImageView
                        android:id="@+id/partTrackIcon"
                        android:layout_width="@dimen/_25dp"
                        android:layout_height="@dimen/_35dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/_30dp"
                        android:background="@drawable/expendrighticon">

                    </ImageView>
                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_15dp"></LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>